@ViewBag.Message

<!--== Header Area End ==-->
<!--== Slider Area Start ==-->

<section id="slider-area">
    <!--== slide Item One ==-->
    <div class="single-slide-item overlay">
        <div class="container">
            <div class="row">
                <div class="col-lg-5">

                    <div class="book-a-car">
                        <form method="post">
                            <!--== Pick Up Location ==-->
                            <div class="pickup-location book-item">
                                <h4>PICK-UP LOCATION:</h4>
                                <select class="custom-select" name="VendorId">
                                    <option selected>Select</option>
                                    @foreach (var item in Model.Vendors)
                                    {
                                        <option value="@item.Id">@item.Adress</option>
                                    }


                                </select>
                            </div>
                            <!--== Pick Up Location ==-->
                            <!--== Pick Up Date ==-->
                            <div class="pick-up-date book-item">
                                <h4>Max Price</h4>
                                <input type="number" min="100" step="20" name="MaxPrice" placeholder="Pick Up Date" />


                            </div>
                            <!--== Pick Up Location ==-->
                            <!--== Car Choose ==-->
                            <div class="choose-car-type book-item">
                                <h4>CHOOSE CAR TYPE:</h4>
                                <select class="custom-select" name="brandId">
                                    <option selected>Select</option>
                                    @foreach (var item in Model.CarBrands)
                                    {
                                        <option value="@item.Id">@item.BrandName</option>

                                    }
                                </select>
                            </div>
                            <!--== Car Choose ==-->

                            <div class="book-button text-center">
                                <button class="book-now-btn">Book Now</button>
                            </div>
                        </form>
                    </div>
                </div>

                <div class="col-lg-7 text-right">
                    <div class="display-table">
                        <div class="display-table-cell">
                            <div class="slider-right-text">
                                <h1>BOOK A CAR TODAY!</h1>
                                <p>FOR AS LOW AS $10 A DAY PLUS 15% DISCOUNT <br> FOR OUR RETURNING CUSTOMERS</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--== slide Item One ==-->
</section>


<!--== Slider Area End ==-->
